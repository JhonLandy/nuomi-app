{
  "name": "desktop-app",
  "version": "1.0.0",
  "description": "桌面应用",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite --mode development --host",
    "build": "cross-env NODE_ENV=production vite --mode production build && electron-builder",
