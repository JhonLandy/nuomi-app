    "vue-eslint-parser": "^9.1.0"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "electron-debug": "^3.2.0",
    "electron-debugger": "^2.1.2",
    "pina": "^0.20.2204228",
    "ramda": "^0.28.0",
    "vue": "^3.2.41",
    "vuetify": "3.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
